{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/auth",
            "dest": "./index.js"
        }
    ],
    "headers": [
        {
            "value": "*"
        },
        {
            "value": "GET, POST"
        },
        {
            "value": "Origin, X-Requested-With, Content-Type, Accept"
        }
    ]
}